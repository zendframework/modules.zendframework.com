{
    "name": "zendframework/modules-site",
    "description": "ZF2 Modules Site",
    "license": "BSD-3-Clause",
    "homepage": "https://modules.zendframework.com",
    "require": {
        "php": "~5.4",
        "bshaffer/oauth2-server-php": "dev-develop",
        
        "zendframework/zendframework": "~2.3.0",
        "zfcampus/zf-apigility": "~1.0.0",
        "zfcampus/zf-apigility-provider": "~1.0.0",
        "zfcampus/zf-apigility-documentation": "~1.0.0",
        "zfcampus/zf-api-problem": "~1.0.0",
        "zfcampus/zf-content-negotiation": "~1.0.0",
        "zfcampus/zf-content-validation": "~1.0.0",
        "zfcampus/zf-development-mode": "~2.0",
        "zfcampus/zf-hal": "~1.0.0",
        "zfcampus/zf-mvc-auth": "~1.0.0",
        "zfcampus/zf-oauth2": "~1.0.0",
        "zfcampus/zf-rest": "~1.0.0",
        "zfcampus/zf-rpc": "~1.0.0",
        "zfcampus/zf-versioning": "~1.0.0",
        "rwoverdijk/assetmanager": "1.3.*",
        "evandotpro/edp-github": "dev-master",
        "evandotpro/edp-markdown": "0.2.*",
        "evandotpro/edp-module-layouts": "1.0.*",
        "lorenzoferrarajr/lfj-error-layout": "1.0.*",
        "zf-commons/zfc-user": "0.1.*",
        "zf-commons/zfc-base": "0.1.*",
        "socalnick/scn-social-auth": "1.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "squizlabs/php_codesniffer": "~2.0.0@RC",
        "zendframework/zftool": "dev-master",
        "zendframework/zend-developer-tools": "dev-master",
        "zfcampus/zf-apigility-admin": "~1.0-dev",
        "zfcampus/zf-configuration": "~1.0-dev",
        "zfcampus/zf-apigility-welcome": "~1.0-dev"
    },
    "autoload": {
        "psr-0": {
            "Application": "./module/Application/src/",
            "User": "./module/User/src/",
            "ZfModule": "./module/ZfModule/src/"
        },
        "classmap": [
            "./vendor/hybridauth/hybridauth/hybridauth/Hybrid",
            "./vendor/hybridauth/hybridauth/additional-providers/hybridauth-github/Providers"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "ApplicationTest": "./module/Application/test/",
            "ZfModuleTest": "./module/ZfModule/test/"
        }
    },
    "minimum-stability": "dev"
}

